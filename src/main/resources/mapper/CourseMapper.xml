<?xml version="1.0" encoding="UTF-8"?>
<!--mapper的xml的头文件-->
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wyl.dao.CourseDao">
    <select id="getAllById" resultType="com.wyl.pojo.Course">
        select * from course where cno=#{cno}
    </select>
    <select id="getAllCourse" resultType="com.wyl.pojo.Course">
        select * from course
    </select>
</mapper>
